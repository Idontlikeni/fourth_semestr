    "from functools import cache\n",
    "\n",
    "@cache\n",
    "def lev_dist(a, b, i=-1, j=-1):\n",
    "    if i == -1:\n",
    "        i = len(a)\n",
    "        j = len(b)\n",
    "    if i == 0 or j == 0:\n",
    "        return max(i, j)\n",
    "    if a[i - 1] == b[j - 1]:\n",
    "        return lev_dist(a, b, i - 1, j - 1)\n",
    "    return 1 + min(lev_dist(a, b, i - 1, j), lev_dist(a, b, i, j - 1), lev_dist(a, b, i - 1, j - 1))\n",
    "\n",
    "lev_dist('столб', 'слон')\n"
